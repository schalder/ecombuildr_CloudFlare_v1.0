{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/catchall/index.ts": {
      "runtime": "edge"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/catchall",
      "has": [
        {
          "type": "host",
          "value": "(?!.*ecombuildr\\.com$).*"
        }
      ]
    }
  ]
}
