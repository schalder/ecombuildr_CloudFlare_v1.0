{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/[...catchall]",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "facebookexternalhit"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/api/[...catchall]",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
